package SelfProgramming;

public class IfElse {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int time = 11;
		 if (time > 7 ) {
		 System.out.println("Good Morning.");
	   } else if (time < 23) {
		 System.out.println("Good day.");
	   } else {
		 System.out.println("Good Evening.");
		    }  

	}

}

OUTPUT

0
15
37
47
9
16
-8
9
