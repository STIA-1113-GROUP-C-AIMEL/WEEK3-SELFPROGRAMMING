package SelfProgramming;

public class Boolean {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		 boolean Amihandsome = true;
		 boolean iloveyou = false; 
		 int x = 10;
		 int y = 12;
		 
		 System.out.println( Amihandsome);
		 System.out.println(iloveyou);
		 System.out.println(x > y);  //for false
		 System.out.println(x < y);  //for true
		 System.out.println(x == 12); //for false
		 System.out.println(x == 10); //for true

	}

}

OUTPUT

true
false
false
true
false
true
