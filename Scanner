package personal;

import java.util.Scanner;

public class Import {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		  int number1, number2, sum, sum2;
		  double cgpa;
		  String name;
		  char gred;
	  
	     
	      Scanner scan = new Scanner (System.in);
	      
	      System.out.println("Enter your name:");
	      name = scan.nextLine();
	      
	      System.out.println("Enter your math gred for SPM:");
	      gred = scan.next().charAt(0);
	      
	      System.out.println("Enter your CGPA = ");
	      cgpa = scan.nextDouble();
	      
	      System.out.println("Enter Number 1:");
	      number1 = scan.nextInt();
	      System.out.println("Enter Number 2:");
	      number2 = scan.nextInt();
	      
	      
	      System.out.println("My name is " + name);
	      System.out.println("Math Gred ="+ gred);
	      System.out.println("My CGPA is " + cgpa);
	      System.out.println("Number1 = "+ number1);
	      System.out.println("Number2 = "+ number2);
	     
	      
	      sum = number1 + number2;
	      sum2 = number2 - number1;
	     
	      
	      String txt = "The sum is ";
	      
	      System.out.println( txt + sum );
	      System.out.println( txt  + sum2);
	   
	}

}
 OUTPUT

Enter your name:
AIMEL
Enter your math gred for SPM:
A
Enter your CGPA = 
3.33
Enter Number 1:
9
Enter Number 2:
10
My name is AIMEL
Math Gred =A
My CGPA is 3.33
